{
  "name": "chumi-vite",
  "version": "1.0.0",
  "repository": "git@github.com:chumijs/chumi-vite.git",
  "author": "topthinking",
  "license": "MIT",
  "scripts": {
    "dev": "prisma generate && vite dev",
    "build:web": "vite build -c vite.config.web.ts",
    "build:server": "prisma generate && vite build -c vite.config.node.ts",
    "build": "rm -rf dist && npm run build:web && npm run build:server",
    "start": "node dist",
    "lint": "eslint src server --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "chumi": "^1.0.0",
    "koa": "^2.14.1",
    "koa-body": "^5.0.0",
    "koa-compose": "^4.1.0",
    "koa-router": "^10.1.1"
  },
  "devDependencies": {
    "@prisma/client": "^4.11.0",
    "@swc/core": "^1.2.177",
    "@types/fs-extra": "^11.0.1",
    "@types/koa": "^2.13.4",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-mount": "^4.0.1",
    "@types/koa-router": "^7.4.4",
    "@types/koa-session": "^5.10.6",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^18.13.0",
    "@types/rollup-plugin-node-builtins": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "fs-extra": "^11.1.0",
    "http-proxy-middleware": "^2.0.6",
    "koa-compress": "^5.1.0",
    "koa-mount": "^4.0.0",
    "koa-session": "^6.2.0",
    "koa-static": "^5.0.0",
    "koa2-connect": "^1.0.2",
    "prettier": "^2.6.2",
    "prisma": "^4.11.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "sass": "^1.58.1",
    "terser": "^5.16.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4",
    "vite": "3",
    "vite-plugin-node": "^3.0.2",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  }
}
